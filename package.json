{
  "name": "@kainstar/eslint-config",
  "version": "1.0.2",
  "description": "My personal eslint config",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "cm": "cz",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kainstar/eslint-config.git"
  },
  "license": "MIT",
  "author": {
    "name": "Kainstar",
    "email": "15249633+kainstar@users.noreply.github.com",
    "url": "https://github.com/kainstar"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "kainstar"
  ],
  "bugs": {
    "url": "https://github.com/kainstar/eslint-config/issues"
  },
  "homepage": "https://github.com/kainstar/eslint-config#readme",
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@eslint-types/typescript-eslint": "^6.18.1",
    "@eslint/js": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.20.1",
    "eslint-plugin-yml": "^1.11.0",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^13.24.0",
    "local-pkg": "^0.5.0",
    "vue-eslint-parser": "^9.4.0",
    "yaml-eslint-parser": "^1.2.2"
